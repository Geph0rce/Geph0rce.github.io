  <entry>
    <title>Git Tips</title>
    <link href="https://geph0rce.github.io/2017/12/15/2017-12-15-git-tips/"/>
    <id>https://geph0rce.github.io/2017/12/15/2017-12-15-git-tips/</id>
    <published>2017-12-14T16:00:00.000Z</published>
